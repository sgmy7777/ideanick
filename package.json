   "devDependencies": {
      "concurrently": "^8.2.0",
      "prettier": "3.0.1",
      "rimraf": "5.0.1",
      "ts-node": "10.9.1",
      "ts-node-dev": "2.0.0",
      "typescript": "5.1.6"
   }
}