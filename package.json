{
   "name": "ideanick",
   "private": true,
   "version": "0.0.0",
   "scripts": {
      "b": "pnpm --filter @ideanick/backend",
      "w": "pnpm --filter @ideanick/webapp",
      "dev": "pnpm -r --parallel dev",
      "types": "concurrently 'pnpm b types' 'pnpm w types'",
      "lint": "eslint --no-error-on-unmatched-pattern --ext .ts,.tsx,.js,.jsx backend/src webapp/src",
      "lint:fix": "eslint --fix --no-error-on-unmatched-pattern --ext .ts,.tsx,.js,.jsx backend/src webapp/src",
      "lint:b": "eslint --no-error-on-unmatched-pattern --ext .ts backend/src",
      "lint:w": "eslint --no-error-on-unmatched-pattern --ext .ts,.tsx webapp/src",
      "prettify": "pnpm -r --parallel prettify"
   },
   "devDependencies": {
      "@typescript-eslint/eslint-plugin": "5.62.0",
      "@typescript-eslint/parser": "5.62.0",
      "concurrently": "^8.2.0",
      "eslint": "8.47.0",
      "eslint-config-prettier": "9.0.0",
      "eslint-config-react-app": "7.0.1",
      "eslint-config-standard-with-typescript": "37.0.0",
      "husky": "8.0.3",
      "lint-staged": "14.0.0",
      "prettier": "3.0.1",
      "rimraf": "5.0.1",
      "ts-node": "10.9.1",
      "ts-node-dev": "2.0.0",
      "typescript": "5.1.6"
   }
}